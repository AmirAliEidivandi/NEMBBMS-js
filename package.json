{
  "name": "nodejs-backend-structure-modular-js",
  "version": "1.0.0",
  "description": "Node JS Backend Application in Modular and structured way",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest",
    "db:up": "docker run -d --name NEMBBMS-JS -p 27501:27017 -e MONGO_INITDB_ROOT_USERNAME=archetype -e MONGO_INITDB_ROOT_PASSWORD=archetype mongo",
